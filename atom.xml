---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>garrett.im</title>
  <link href="http://garrett.im/"/>
  <link type="application/atom+xml" rel="self" href="http://garrett.im/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://garrett.im/</id>
  <author>
    <name>Garrett Robinson</name>
    <email>garrett.f.robinson@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://garrett.im{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://garrett.im{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Garrett Robinson</name>
      <uri>http://garrett.im</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
